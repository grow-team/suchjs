{
  "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "outDir": "./lib",
      "sourceMap": true,
      "lib": [
          "es2017",
          "dom"
      ]
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules",
      "lib/*",
      "**/*.spec.ts"
  ],
  "baseUrl": ".",
  "paths":{
      "@":"src/"
  }
}